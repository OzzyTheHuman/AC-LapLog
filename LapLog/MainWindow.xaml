<Window x:Class="LapLog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LapLog"
        xmlns:views="clr-namespace:LapLog.Views"
        xmlns:vms="clr-namespace:LapLog.ViewModel"
        mc:Ignorable="d"
        Title="LapLog" Height="450" Width="800"
        FontSize="14">
        
        <Grid Margin="20 10" MaxWidth="600">
                <Grid.Resources>
                        <DataTemplate DataType="{x:Type vms:LapTimeListingViewModel}">
                                <views:LapTimeListingView/>
                        </DataTemplate>
                </Grid.Resources>
                
                <ContentControl Content="{Binding}"></ContentControl>
        </Grid>
</Window>
